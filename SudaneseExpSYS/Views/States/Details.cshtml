@model SudaneseExpSYS.Models.State

@{
    ViewData["Title"] = "Details";
}

<div class="title-header">
    <h4 class="d-flex justify-content-center "><i class="mx-2 my-auto fa fa-globe"> @SudaneseExpSYS.Resourses.Resource.States</i></h4>
</div>

<fieldset class="card p-4 shadow" dir="rtl">
    <legend class="bg-custom shadow  px-3">@SudaneseExpSYS.Resourses.Resource.Details</legend>
    <div class="row">
        <div class="alert col-md-5 mx-4 alert-info ">
            <label asp-for="SName" class="control-label">@SudaneseExpSYS.Resourses.Resource.SName</label>
            <div class="">
                @Html.DisplayFor(model => model.SName)
            </div>
        </div>

    </div>

    <p>
        <a asp-action="Edit" class="btn btn-primary">@SudaneseExpSYS.Resourses.Resource.Edit</a>
        <a asp-action="Index" class="btn btn-info">@SudaneseExpSYS.Resourses.Resource.Back</a>
    </p>
</fieldset>


