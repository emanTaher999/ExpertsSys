@model SudaneseExpSYS.Models.Country

@{
    ViewData["Title"] = "Delete";
}
<div class="title-header">
    <h4 class="d-flex justify-content-center "><i class="mx-2 my-auto fa fa-globe"> @SudaneseExpSYS.Resourses.Resource.Country</i></h4>
</div>
<br />

<fieldset class="card p-4 shadow">

    <legend class="bg-custom shadow  px-3">@SudaneseExpSYS.Resourses.Resource.Delete</legend>
    <br />
    <form asp-action="Delete">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="CId" />
        <div class="row my-2">
            <div class="col-md-3">

                <label asp-for="CName" class="control-label">@SudaneseExpSYS.Resourses.Resource.CName</label>
                <div class="editor-field">
                    <input asp-for="CName" class="form-control" />
                    <span asp-validation-for="CName" class="text-danger"></span>
                </div>
            </div>


        </div>
        <br />
        <p>
            <input type="submit" value="@SudaneseExpSYS.Resourses.Resource.Delete" class="btn btn-danger" />
            <a asp-action="Index" class="btn btn-info">@SudaneseExpSYS.Resourses.Resource.Back</a>

        </p>
    </form>
</fieldset>
